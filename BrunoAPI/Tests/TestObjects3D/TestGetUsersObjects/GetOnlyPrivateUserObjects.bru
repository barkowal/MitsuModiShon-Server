meta {
  name: GetOnlyPrivateUserObjects
  type: http
  seq: 3
}

get {
  url: {{local}}:3000/api/v1/objects3D/private?public=false
  body: none
  auth: inherit
}

params:query {
  public: false
}

assert {
  res.status: eq 200
}

tests {
  test("should return only private user objects", function () {
    let res_objects = res.getBody().data.result.objects;
    let is_public = false;
    for(let obj of res_objects){
      if(obj.isPublic){
        is_public = true;
        break;
      }
    }
    let other_user = false;
    for(let obj of res_objects){
      if(obj.username !== "Test_Account"){
        other_user = true;
        break;
      }
    }
    
    expect(is_public || other_user).to.be.false;
  });
}

settings {
  encodeUrl: true
}
