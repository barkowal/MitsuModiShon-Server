meta {
  name: GetUserObjectsWithoutPagination
  type: http
  seq: 1
}

get {
  url: {{local}}:3000/api/v1/objects3D/private
  body: none
  auth: inherit
}

assert {
  res.status: eq 200
}

tests {
  test("should return success", function () {
    let res_success = res.getBody().success;
    expect(res_success).to.equal(true);
  });
  
  test("should return correct message", function () {
    let message = res.getBody().message;
    expect(message).to.equal("Found objects successfully.");
  });
  
  test("should return only user objects", function () {
    let res_objects = res.getBody().data.result.objects;
    let other_user = false;
    for(let obj of res_objects){
      if(obj.username !== "Test_Account"){
        other_user = true;
        break;
      }
    }
    expect(other_user).to.be.false;
  });
}

settings {
  encodeUrl: true
}
