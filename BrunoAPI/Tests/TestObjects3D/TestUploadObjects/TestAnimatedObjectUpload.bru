meta {
  name: TestAnimatedObjectUpload
  type: http
  seq: 9
}

post {
  url: {{local}}:3000/api/v1/objects3D/upload
  body: multipartForm
  auth: inherit
}

body:multipart-form {
  objectData: {"name":"test_animated_object","is_public":true,"is_animated":true}
  image: @file(testFiles/test_animated_render.png)
  object3D: @file(testFiles/test_animated_mesh.json)
}

assert {
  res.status: eq 200
}

settings {
  encodeUrl: true
  timeout: 0
}
