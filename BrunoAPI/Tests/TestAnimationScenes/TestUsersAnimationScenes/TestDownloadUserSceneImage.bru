meta {
  name: TestDownloadUserSceneImage
  type: http
  seq: 8
}

get {
  url: {{local}}:3000/api/v1/image{{signed_image_path}}
  body: none
  auth: inherit
}

assert {
  res.status: eq 200
}

tests {
  test("should return image", function () {
    let res_header = res.getHeader("content-type");
  
    expect(res_header).to.be.oneOf(["image/png","image/jpg"]);
  });
}

settings {
  encodeUrl: true
}
